<p id="notice"><%= notice %></p>

<h1>Club Directors</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @club_directors.each do |club_director| %>
      <tr>
        <td><%= link_to 'Show', club_director %></td>
        <td><%= link_to 'Edit', edit_club_director_path(club_director) %></td>
        <td><%= link_to 'Destroy', club_director, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Club Director', new_club_director_path %>
